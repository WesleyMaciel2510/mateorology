{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "baseUrl": "./",
    "lib": ["ES6", "ES2015"],
    "paths": {
      "@services/*": ["services/*"]
    },
    "esModuleInterop": true,
    "types": ["react-native-fs", "use-between"],
    "resolveJsonModule": true // Add this line to enable JSON module resolution
  },
  "jsx": "react-native"
}
